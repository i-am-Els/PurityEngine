{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "source": {
      "type": "string",
      "enum": [ "asset", "prefab", "scene" ]
    },
    "type_": {
      "type": "string",
      "enum": [
        "TextureAsset",
        "SpriteAsset",
        "ShaderAsset",
        "MeshAsset",
        "SkeletonAsset",
        "StaticMeshAsset",
        "MaterialAsset",
        "SplineAsset",
        "PolygonMeshAsset",
        "SoundSourceAsset",
        "3DSoundSourceAsset",
        "LevelAsset",
        "ParticleAssets",
        "RenderMapAssets"
      ]
    },
    "data_": {
      "$ref": "#/definitions/Data"
    },
    "ref_assets": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Ref_asset"
      }
    }
  },
  "required": ["id", "source", "type_", "data_"],
  "definitions": {
    "Data": {
      "type": "object"
    },
    "Ref_asset": {
      "type": "string"
    }
  }
}